SELECT namefirst, namelast, sb + cs AS stolen_base_attempt, 
		ROUND((sb * 100)::numeric/ (sb + cs), 2) AS percent_stolen_success
FROM batting
INNER JOIN people
ON batting.playerid = people.playerid
WHERE yearid = 2016
GROUP BY namefirst, namelast, sb, cs
HAVING sb + cs >= 20
ORDER BY ((sb * 100) / (sb + cs)) DESC
LIMIT 1
